{% extends "base.html.twig" %}

{% block body %}
	<div class="container">
		<h1>Mes Squads</h1>
		<div class="row">
			{% for item in squad %}
				<div class="col">
					<div class="row">
						{{item.name}}
					</div>
					<div class="row">
						{{item.id}}
					</div>
				</div>
			{% endfor %}
		</div>
		<a href="{{path('app_squad_add')}}">Nouvelle Squad</a>
	</div>

{% endblock %}
